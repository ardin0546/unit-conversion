{
    "name": "spatie/ardin-unit-conversion",
    "description": "Hello world",
    "keywords": [
        "spatie",
        "ardin-unit-conversion"
    ],
    "homepage": "https://github.com/spatie/ardin-unit-conversion",
    "license": "MIT",
    "authors": [
        {
            "name": "ardin0546",
            "email": "ardin468@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ArdinUnitConversion\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ArdinUnitConversion\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
